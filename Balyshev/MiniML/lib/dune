(library
 (name MiniML)
 (public_name MiniML.lib)
 (modules Ast Monads Parser Interpreter Inferencer)
 (modes native byte)
 (flags
  (:standard -g))
 (libraries base angstrom str)
 (preprocess
  (pps ppx_deriving.show))
 (instrumentation
  (backend bisect_ppx)))

(library
 (name tests)
